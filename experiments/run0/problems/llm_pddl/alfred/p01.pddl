(define (problem credit-card-container)
  (:domain household)
  (:objects container credit-card - object desk - location)
  (:init
    (handempty hand1)
    (not (found credit-card))
    (ontable container)
    (open container)
    (on desk desk)
  )
  (:goal (and (holding hand1 credit-card) (on credit-card desk)))
)